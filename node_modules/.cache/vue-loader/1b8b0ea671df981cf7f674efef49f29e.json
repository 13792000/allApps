\n    }\n  },\n  data() {\n    return {\n      icon: 'chevron-down',\n      showe: false,\n      items:[\n      { \n        name: 'text 1',\n        id: 1,\n        parentId : null,\n      },\n      {\n        name: 'text 2',\n        id: 2,\n        parentId : null,\n      },\n      {     \n        name: 'text 3',\n        id: 3,\n        parentId: 2,\n      },\n      {\n        name: 'text 4',\n        id: 4,\n        parentId: 2,\n      },\n      { \n        name: 'text 5',\n        id: 5,\n        parentId: 4, \n      }\n    ]\n  }\n},\n  components:{\n    Item,\n  },\n  methods: {\n    chekIcon(id){\n      const showe = this.items.filter(f => id == f.parentId)\n      return showe.length\n    },\n    showeItem(){\n      if(this.icon == 'chevron-down'){\n       this.showe = true\n       this.icon = 'chevron-up'\n      }\n      else {\n       this.showe = false\n       this.icon = 'chevron-down' \n      }\n    }\n  },\n \n}\n</script>\n"]}]}